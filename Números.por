programa {
  funcao inicio() {
    inteiro numero, quadrado

    escreva("Informe um número: ")
    leia(numero)

    enquanto(numero!=0){
      escreva(" Informe o novo número: ")
      leia (numero)
      quadrado=numero*numero
   
     escreva("O número ao quadrado " +numero+ " é ", +quadrado)
      }
    }
  }
}
